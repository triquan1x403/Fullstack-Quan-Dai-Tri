{
  "name": "express-js",
  "version": "1.0.0",
  "main": "",
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json}",
    "build": "npx tsc --outDir dist/ && npx copyfiles -u 1 src/**/*.* dist/",
    "build:lean": "npx tsc --skipLibCheck --noEmit --esModuleInterop --outDir dist/",
    "start:dev": "nodemon src/main.ts",
    "start": "npx ts-node src/main.ts",
    "start:prod": "node dist/main.js",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --edit",
    "schema:check": "npx zenstack check --schema=src/databases/schema.zmodel",
    "schema:format": "npx zenstack format --schema=src/databases/schema.zmodel",
    "schema:generate": "npx zenstack generate --schema=src/databases/schema.zmodel && npx prisma generate --schema=src/databases/prisma/schema.prisma",
    "schema:reset": "npm run schema:generate && npx prisma migrate reset --schema=src/databases/prisma/schema.prisma",
    "migration:generate": "npm run schema:check && npm run schema:format && npm run schema:generate && npx prisma migrate dev --schema=src/databases/prisma/schema.prisma --create-only --name",
    "migration:dev": "npx prisma migrate dev --schema=src/databases/prisma/schema.prisma",
    "migration:prod": "npx prisma migrate deploy --schema=src/databases/prisma/schema.prisma",
    "migration:status": "npx prisma migrate status --schema=src/databases/prisma/schema.prisma",
    "seed:demo": "npx ts-node src/databases/seeds/demo.seed.ts"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@zenstackhq/runtime": "2.10.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-error-handler": "^1.1.0",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^6.1.0",
    "swagger-ui-express": "^5.0.1",
    "yaml": "^2.6.0",
    "zenstack": "2.10.2",
    "zod": "^3.23.8",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/format": "^19.5.0",
    "@eslint/eslintrc": "^3.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.9",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "commitlint": "^19.5.0",
    "cspell": "^8.15.4",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.6.3"
  }
}
